{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "completed-docs": [
      false
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-no-multiline-js": false,
    "no-any": true
  }
}