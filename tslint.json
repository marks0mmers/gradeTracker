{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "linterOptions": {
        "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "completed-docs": [
            false
        ],
        "interface-name": false,
        "jsx-no-multiline-js": false,
        "no-any": true,
        "object-literal-sort-keys": false,
        "variable-name": false,
        "arrow-return-shorthand": true
    }
}